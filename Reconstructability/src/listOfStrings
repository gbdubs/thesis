typedef struct ListOfStrings{
	char* myStr;
	int size;
	ListOfStrings* next;
} LOS;

LOS* newListOfStrings(char* s){
	LOS* newLos = malloc(sizeof(LOS));
	newLos->next = NULL;
	newLos->size = 1;
	newLos->myStr = s;
	return newLos;
}

LOS* addToListOfStrings(LOS* los, char* s){
	LOS* newLos = malloc(sizeof(LOS));
	newLos->next = los;
	newLos->size = los->size + 1;
	newLos->myStr = s;
	return newLos;
}


int main(){
	printf("hello world");
}